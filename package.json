  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tridecco/game-board.git"
  },
  "license": "AGPL-3.0",
  "author": "Tony Kan",
  "type": "commonjs",
  "main": "./src/index.js",
  "browser": "./dist/tridecco-board.min.js",
  "scripts": {
